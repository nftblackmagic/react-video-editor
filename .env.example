# Bytescale Configuration
# Get these values from: Bytescale Dashboard

# For server-side API routes (keep these secret)
BYTESCALE_ACCOUNT_ID=your_account_id_here  # e.g., W142hJk
BYTESCALE_API_KEY=your_api_key_here  # Your Bytescale API key for authentication

# For client-side uploads (these will be exposed to the browser)
NEXT_PUBLIC_BYTESCALE_ACCOUNT_ID=your_account_id_here  # Same as BYTESCALE_ACCOUNT_ID
NEXT_PUBLIC_BYTESCALE_API_KEY=your_api_key_here  # Same as BYTESCALE_API_KEY

# Other existing environment variables
PEXELS_API_KEY=""
COMBO_SH_JWT=""

# Transcription Service
# ElevenLabs API key for transcription (server-side)
ELEVENLABS_API_KEY=""
# Optional: Client-side key if needed
NEXT_PUBLIC_ELEVENLABS_API_KEY=""

DATABASE_URL="postgresql://localhost:5432/video_editor"